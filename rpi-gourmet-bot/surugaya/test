var gSecUrl = "https://secure.amiami.jp/";
var gGcode = "FIGURE-019822";
var gScode = "FIGURE-019822";
var gPagePath = "top/detail/detail";
var gWishSuccessMsgFunc = WishSuccessMsgFunc;
var gWishFailureMsgFunc = WishFailureMsgFunc;
function WishSuccessMsgFunc(){
	$(".wishlist_message span").html("お気に入りに登録しました。");
	$("#glayLayer").show();
	$("#overLayer").show();
}
function WishFailureMsgFunc(){
	$(".wishlist_message span").html("お気に入り登録でエラーが発生しました。");
	$("#glayLayer").show();
	$("#overLayer").show();
}
$(function(){
	$("body").append("<div id='glayLayer'></div><div id='overLayer'><div class='wishlist_close'></div><div class='wishlist_message'><span></span></div></div>");
	$("#glayLayer").click(function(){$("#glayLayer").fadeOut(100);$("#overLayer").fadeOut(100);});
	$("#overLayer").click(function(){$("#glayLayer").fadeOut(100);$("#overLayer").fadeOut(100);});
});

var gProductLikeSuccessFunc = ProductLikeSuccessFunc;
var gProductLikeFailureFunc = ProductLikeFailureFunc;
var gProductLikeNotLoginFunc = ProductLikeNotLoginFunc;
function ProductLikeSuccessFunc(like){
	if (like === 1) {
		$("#productlike_request_").attr("id", "productlike_request_cancel");
		$("#productlike_request_cancel").html('<img src="http://www.amiami.jp/images/parts/like_on.png" width="22" height="22" />');
		$("#like_count").text(Number($("#like_count").text())+1);
	} else if (like === 0) {
		$("#productlike_request_cancel").attr("id", "productlike_request_");
		$("#productlike_request_").html('<img src="http://www.amiami.jp/images/parts/like_off.png" width="22" height="22" />');
		$("#like_count").text(Number($("#like_count").text())-1);
	}
}
function ProductLikeFailureFunc(like){
	if (like === 1) {
		alert("エラーが発生しました。");
	} else if (like === 0) {
		alert("エラーが発生しました。");
	}
}
function ProductLikeNotLoginFunc(like){
	if (like === 1) {
		alert("ログインが必要です。");
	} else if (like === 0) {
		alert("ログインが必要です。");
	}
}

$("<img>").attr("src", "http://www.amiami.jp/images/parts/like_on.png" );
$("<img>").attr("src", "http://www.amiami.jp/images/parts/like_off.png" );

