		$(function(){
			$(".grouptitle").click(
				function() {
					if($(this).hasClass("selected")){
						$(this).removeClass("selected");
					}else{
						$(this).addClass("selected");
					}
					var num	= $(".grouptitle").index(this);
					$("#browsenode dd ul.grouplist").eq(num).slideToggle("fast");
				}
			);
		});
	</script>
